!!!5
%html{lang: 'en'}
  %head
    %meta{charset: 'utf-8'}
    %title Geek Bank
    %link{rel: 'stylesheet', href: 'css/normalize.css'}
    %link{rel: 'stylesheet', href: 'css/style.css'}
  %body
    #wrapper
      %h1 Welcome to Geek bank of America
      %input#logoInput{type: 'text', placeholder: 'enter logo url'}
      %input#logoBtn{type: 'button', value: 'create logo'}
      %img#logo
      .clear
      %input#balInput{type: 'text', placeholder: 'enter your balance'}
      %input#balBtn{type: 'button', value: 'create balance'}
      .clear
      %input#amount{type: 'text', placeholder: 'enter amount'}
      %input#depositBtn{type: 'button', value: 'DEPOSIT'}
      %input#withdrawBtn{type: 'button', value: 'WITHDRAW'}
      .clear
      %h1 Transaction List
      #transactions
        #depOutput
        %input#balance{type: 'text', value:'$'}
        #withOutput
    %script{src: 'js/jquery-1.10.2.min.js'}
    %script{src: 'js/underscore-min.js'}
    %script{src: 'js/main.js'}
